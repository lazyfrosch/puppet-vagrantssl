{
  "name": "lazyfrosch-vagrantssl",
  "version": "0.1.0",
  "author": "Markus Frosch <markus@lazyfrosch.de>",
  "summary": "Puppet SSL CA between Vagrant boxes",
  "license": "GPL-2.0+",
  "source": "https://github.com/lazyfrosch/puppet-vagrantssl.git",
  "project_page": "https://github.com/lazyfrosch/puppet-vagrantssl",
  "issues_url": "https://github.com/lazyfrosch/puppet-vagrantssl/issues",
  "requirements": [
    { "name": "puppet", "version_requirement": ">= 3.8.0 < 5.0.0" }
  ],
  "dependencies": [
    { "name": "puppetlabs/stdlib", "version_requirement": ">= 4.10.0 < 5.0.0" }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ]
}
